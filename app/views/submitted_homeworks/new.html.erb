<%= render partial: 'home/global', locals: {section: "#{@sHomework.homework.title} 제출"} %>

<div class="container">
  <div class="submitted-homework-section">
    <div class="row">
      <div class="col-xs-12 col-md-4 col-md-offset-4">
        <%= image_tag "", id: "preview-img", class: "preview-img" %>
      </div>
    </div>
    <%= form_for [@sHomework.homework, @sHomework], html: { multipart: true, class: "form-group" } do |f| %>
      <%= render partial: 'form', locals: { sHomework: @sHomework, f: f } %>
    <% end %>
  </div>
</div>
